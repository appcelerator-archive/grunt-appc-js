{
	"browser": true,
	"camelcase": false,
	"curly": true,
	"eqeqeq": true,
	"eqnull": true,
	"expr": true,
	"immed": true,
	"indent": 4,
	"latedef": false,
	"newcap": true,
	"noarg": true,
	"nonew": true,
	"undef": true,
	"unused": false,
	"loopfunc": true,
	"proto": true,
	"node": true,
	"-W104": true,
	"-W068": true,
	"globals": {
		"describe"   : false,
		"it"         : false,
		"before"     : false,
		"beforeEach" : false,
		"after"      : false,
		"afterEach"  : false
	}
}